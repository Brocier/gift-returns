<div>
    <h1>Which user are you?</h1>
    <div>
        <a href="/users/new">Add a New User</a>
    </div>
    {{#each users}}
        <div>
            <span>
                <img src="{{this.photoUrl}}" alt="{{this.username}}'s profile image">
            </span>
            <span>
                <a href="/users/{{this._id}}">{{this.firstName}} {{this.lastName}}</a>
            </span>
        </div>
    {{/each}}
</div>
